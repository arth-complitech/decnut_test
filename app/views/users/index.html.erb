<p id="notice"><%= notice %></p>

<b><%=link_to "Logout" ,destroy_user_session_path , method: :delete %></b>
<h1>Listing Users</h1>



<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Mobile Number</th>
      <th>Type</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.mobile_number %></td>
        <td><%= user.type %></td>
        <td><%= user.email %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_registration_path %>
